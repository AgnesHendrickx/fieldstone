set term pdf enhanced font "Times,10pt"
set grid

set xtics 500
set ytics 1000
set xlabel 'r (km)'
set ylabel 'density'
set output 'rho.pdf'
plot[][0:]\
'rho.ascii' u ($1/1000):2 w l notitle


reset
set key outside
set grid
set xlabel 'h (km)'
set ylabel 'g'
set log x
set output 'g.pdf'
plot[][]\
9.82194 lt -1 ,\
'g.ascii' u ($1/1000):2 w l t 'prem'
