#! /bin/bash

rm -f vrms

for j in 3 4 5 ; do
    echo nmarker_per_dim= $j
for k in 8 10 12 14 16 ; do
    echo nelx=nely= $k
    python3 fieldstone.py $k $k 0 1 $j 0 | grep vrms >> vrms_avrg1_norand
done 
done 

for j in 3 4 5 ; do
    echo nmarker_per_dim= $j
for k in 8 10 12 14 16 ; do
    echo nelx=nely= $k
    python3 fieldstone.py $k $k 0 2 $j 0 | grep vrms >> vrms_avrg2_norand
done 
done 

for j in 3 4 5 ; do
    echo nmarker_per_dim= $j
for k in 8 10 12 14 16 ; do
    echo nelx=nely= $k
    python3 fieldstone.py $k $k 0 3 $j 0 | grep vrms >> vrms_avrg3_norand
done 
done 

