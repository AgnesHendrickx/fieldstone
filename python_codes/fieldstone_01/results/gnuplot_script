set term pdf enhanced font "Times,12pt"
set grid
set xlabel 'x'
set ylabel 'pressure'

set output 'pressure.pdf'
plot[0:1][]\
'pressure.ascii' u 1:3 w p t '32x32',\
x*(1-x)-1./6. lt -1 t 'analytical '





set output 'errors.pdf'
set key outside
set log xy
set xlabel 'h'
set ylabel 'error'
set format y "10^{%L}"
plot[][]\
'errors' u (1./sqrt($3)):6 w lp t 'velocity',\
'errors' u (1./sqrt($3)):9 w lp t 'pressure',\
x**2/10. w l lt -1 dashtype 2 t 'x^2',\
x**1/10. w l lt -1 dashtype 3 t 'x^1'
