set term pdf enhanced
set output 'errors.pdf'
set log xy
set xlabel 'h'
set ylabel 'error'
set key bottom
plot[][]\
'errors3' u (1./sqrt($3)):6 w lp lt 1 pt 1 t 'velocity, nqel=3',\
'errors3' u (1./sqrt($3)):9 w lp lt 2 pt 1 t 'pressure, nqel=3',\
'errors6' u (1./sqrt($3)):6 w lp lt 1 pt 2 t 'velocity, nqel=6',\
'errors6' u (1./sqrt($3)):9 w lp lt 2 pt 2 t 'pressure, nqel=6',\
x**2/11. w l t 'x^2',\
x**1.5 w l t 'x^{1.5}',\
x**1/2 w l t 'x^{1}'
