#! /bin/bash

for res in 1 2 3 4 5; do
    echo res= $res
    sed -i "s/res=.*/res="$res"/" parameters.py
    ./run_script > screen
    mkdir results_$res
    mv screen results_$res
    mv *.ascii results_$res
    mv *.vtu results_$res
done



